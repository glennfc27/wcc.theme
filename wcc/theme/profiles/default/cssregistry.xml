<?xml version="1.0"?>
<object name="portal_css">

 <!-- Set conditions on stylesheets we don't want to pull in -->
 <stylesheet
     expression="python: (not request.get('HTTP_X_THEME_ENABLED', None)) or '@@theming-controlpanel' in request.getURL()"
     id="public.css"
     />
 <stylesheet
     id="wcc-theme-scheme.css"
     expression="python: request.get('HTTP_X_THEME_ENABLED', None)"
     media="screen" rel="stylesheet" rendering="link"
     cacheable="False" compression="safe" cookable="True"
     enabled="1"
     insert-after="public.css"/>

</object>
